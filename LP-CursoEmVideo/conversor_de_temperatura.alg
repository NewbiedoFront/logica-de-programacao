algoritmo "Conversor de Temperatura"
// Função :
// Autor :
// Data : 24/03/2017
// Seção de Declarações
var
   F, C : Real
inicio
   escreva("Qual a temperatura em Washington? ")
   leia(F)

   C <- (F - 32) / 1.8

   escrevaL("")
   escreva("Essa Temperatura no Brasil é: ", C:4, "°C")
   escrevaL("")

fimalgoritmo
