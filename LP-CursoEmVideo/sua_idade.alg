algoritmo "semnome"
// Função :
// Autor :
// Data : 24/03/2017
// Seção de Declarações 
var
   ano_atual, ano_nasc, idade: Inteiro
inicio
   escreva("Em que ano estamos? ")
   leia(ano_atual)
   escreva("Em qual ano você nasceu? ")
   leia(ano_nasc)

   idade <- ano_atual - ano_nasc

   escrevaL("")
   escreva("Uhnm...", idade, " anos?")
   escrevaL("")
   
fimalgoritmo
